{% extends 'base.html.twig' %}

{% block body %}

    <div class="row mt-3">
        <div class="col-lg-6">
            <ul class="list group">
            {% for service in services %}
                <li class="list-group-item d-flex justify-content-between align-items-center">{{ service.name }}
                <span class="badge">{{ service.cost }} kn</span></li>
            {% endfor %}
            </ul>
        </div>
    </div>
{% endblock %}